@{
    ViewBag.Title = "Facebook_register";
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>notetor.com</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="/content/css/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="/content/css/style.css" rel="stylesheet" type="text/css">
    <link href="~/Plugin/mathscribe/jqmath-0.4.3.css" rel="stylesheet" />
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Plugin/mathscribe/jqmath-0.4.3.css" rel="stylesheet" />
    <link href="~/Content/css/responsive-website.css" rel="stylesheet" />
    <link href="~/assets/css/xenon-core.css" rel="stylesheet" />
    <link href="~/Content/css/hover.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.11.2.min.js"></script>
    <style>
        body {
            font-family: arial;
            background: -webkit-linear-gradient(white, #D3D8E8) !important; /* For Safari 5.1 to 6.0 */
            background: -o-linear-gradient(white, #D3D8E8) !important; /* For Opera 11.1 to 12.0 */
            background: -moz-linear-gradient(white, #D3D8E8)!important; /* For Firefox 3.6 to 15 */
            background: linear-gradient(white, #D3D8E8) !important; /* Standard syntax */
            height: auto;
        }
    </style>
</head>
<body>
    <section class="navigation">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-offset-1 col-md-10">
                    <nav class="login-style">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false"><span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span></button>
                            <a class="navbar-brand" href="/Home/Home">
                                <p class="logo">
                                    <img src="/content/images/logo.png" class="img-responsive">
                                </p>
                            </a>
                        </div>
                        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                            <ul class="nav navbar-nav navbar-right">
                                <form>
                                    <div class="col-md-5 col-sm-5">
                                        <div class="form-group">
                                            <label>Email or Phone</label>
                                            <input type="email" class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-md-5 col-sm-5">
                                        <div class="form-group">
                                            <label>Password</label>
                                            <input type="password" class="form-control">
                                            <div class="forget-pass"><a href="#">Forgotten Password?</a></div>
                                        </div>
                                    </div>
                                    <div class="col-md-2 col-sm-2">
                                        <button type="submit" class="btn btn-default login-btn-facebook">log in</button>
                                    </div>
                                </form>
                            </ul>
                        </div>
                    </nav>
                </div>
            </div>
        </div>
    </section>
    <div class="contentaria-division">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-7">
                    <div class="row">
                        <div class="col-md-offset-2 col-md-10">
                            <h2 class="quote">Making  Students life Easier by providing online learning</h2>

                            <img src="~/Content/images/note.png" />
                        </div>
                    </div>
                </div>
                <div class="col-md-5">
                    <h1>Create an account</h1>
                    <h4>It's free and always will be.</h4>
                    <div class="adj clearfix">
                        <div class="form-1st">
                            <div class="form-group">

                                <input type="email" class="form-control" placeholder="First name">
                            </div>
                        </div>
                        <div class="form-2nd">
                            <div class="form-group">

                                <input type="password" class="form-control" placeholder="Last name">
                            </div>
                        </div>
                    </div>
                    <div class="adj clearfix">
                        <div class="form-group">

                            <input type="email" class="form-control" placeholder="Email">
                        </div>
                    </div>
                    <div class="adj clearfix">
                        <div class="form-1st">
                            <div class="form-group">

                                <input type="password" class="form-control" placeholder="Password">
                            </div>
                        </div>
                        <div class="form-2nd">
                            <div class="form-group">

                                <input type="email" class="form-control" placeholder="Confirm password">
                            </div>
                        </div>
                    </div>
                    <div class="adj clearfix">
                        <div class="form-1st">
                            <div class="form-group clearfix">
                                <label class="birthday">Birthday</label>
                                <div>
                                    <select class="all-select">
                                        <option>Day</option>
                                        <option>1</option>
                                        <option>1</option>
                                    </select>
                                    <select class="all-select">
                                        <option>month</option>
                                        <option>1</option>
                                        <option>1</option>
                                    </select>
                                    <select class="all-select">
                                        <option>year</option>
                                        <option>1</option>
                                        <option>1</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="form-2nd">
                            <div class="form-group clearfix">
                                <div class="male-female">
                                    <label class="radio-inline">
                                        <input type="radio" value="option1">
                                        Female
                                    </label>
                                    <label class="radio-inline">
                                        <input type="radio" value="option2">
                                        Male
                                    </label>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="adj clearfix">
                        <div class="form-1st">
                            <div class="form-group">
                                <label class="sr-only">Mobile number</label>
                                <input type="text" class="form-control" placeholder="mobile number">
                                <span class="otp-style"><a href="#">Send OTP </a></span>
                            </div>
                        </div>
                        <div class="form-2nd">
                            <div class="form-group">
                                <label class="sr-only">OTP</label>
                                <input type="text" class="form-control" placeholder="OTP">
                                <span class="otp-style"><a href="#">Verify OTP</a></span>
                            </div>
                        </div>
                    </div>
                    <div class="adj clearfix">
                        <div class="form-group">

                            <input type="text" class="form-control" placeholder="Name Of (Coaching/College/School)">
                        </div>
                    </div>
                    <div class="adj clearfix">
                        <div class="form-group">

                            <input type="file" class="form-control" style="height: auto; padding: 3px 4px;">
                        </div>
                    </div>
                    <div class="adj clearfix">
                        <div class="form1">
                            <div class="form-group">
                                <label class="sr-only">City</label>
                                <select class="all-select address">
                                    <option>City</option>
                                    <option>1</option>
                                    <option>1</option>
                                </select>
                            </div>
                        </div>
                        <div class="form2">
                            <div class="form-group">
                                <label class="sr-only">state</label>
                                <select class="all-select address">
                                    <option>State</option>
                                    <option>1</option>
                                    <option>1</option>
                                </select>
                            </div>
                        </div>
                        <div class="form3">
                            <div class="form-group">
                                <label class="sr-only">Country</label>
                                <select class="all-select address">
                                    <option>Country</option>
                                    <option>1</option>
                                    <option>1</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <input type="button" class="btn-account" value="Create account" />
                    </div>
                </div>
            </div>
        </div>
    </div>

</body>
</html>
<script src="~/Content/js/bootstrap.min.js"></script>
<script src="~/Plugin/mathscribe/jscurry-0.4.0.min.js"></script>
<script src="~/Plugin/mathscribe/jqmath-0.4.3.js"></script>
<script src="~/Scripts/Calculator.js"></script>
<script src="~/Content/js/jquery.slimscroll.min.js"></script>
<script src="~/Content/js/datatables/js/jquery.dataTables.min.js"></script>
<script src="~/Content/js/datatables/dataTables.bootstrap.js"></script>
<script src="~/Content/js/datatables/tabletools/dataTables.tableTools.min.js"></script>
<link href="~/Content/js/datatables/yadcf/jquery.dataTables.yadcf.css" rel="stylesheet" />
